type Company {
  id: ID! @id
  createdAt: DateTime! @createdAt
  ticker: String! @unique
  marketCap: String!
  enterpriseVal: String!
  trailingPE: String!
  forwardPE: String!
  pegRatio: String!
}

type WatchList {
  id: ID! @id
  name: String! @unique
  companies: [Company]
  user: User
}

type User {
  id: ID! @id
  username: String! @unique
  watchLists: [WatchList]
}